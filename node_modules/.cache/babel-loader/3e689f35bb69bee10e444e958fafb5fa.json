{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { v4 as uuidv4 } from 'uuid';\nexport default {\n  data() {\n    return {\n      todos: [{\n        id: \"id1\",\n        content: \"afsdfa\",\n        done: false\n      }, {\n        id: \"id2\",\n        content: \"afsdfa\",\n        done: true\n      }],\n      newToDoContent: \"\"\n    };\n  },\n  methods: {\n    addTodo() {\n      console.log(this.newToDoContent);\n      const newToDo = {\n        id: uuidv4(),\n        content: this.newToDoContent,\n        done: false\n      };\n      console.log(\"new todo:\", newToDo);\n      this.todos.push(newToDo);\n      this.newToDoContent = \"  \";\n    },\n    deleteTodo(itemId) {\n      this.todos.splice(itemId, 1);\n    }\n  }\n};","map":{"version":3,"mappings":";AA6CA,SAAQA,EAAC,IAAKC,MAAK,QAAS,MAAM;AAClC,eAAe;EACXC,IAAI,GAAG;IACP,OAAO;MACHC,KAAK,EAAE,CAAC;QAACC,EAAE,EAAE,KAAK;QAAEC,OAAO,EAAE,QAAQ;QAAEC,IAAI,EAAE;MAAK,CAAC,EAAC;QAACF,EAAE,EAAE,KAAK;QAAEC,OAAO,EAAE,QAAQ;QAAEC,IAAI,EAAE;MAAI,CAAC,CAAC;MAC/FC,cAAc,EAAE;IAChB;EACJ,CAAC;EACDC,OAAO,EAAC;IACJC,OAAO,GAAE;MACLC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACJ,cAAc;MAC/B,MAAMK,OAAM,GAAI;QACZR,EAAE,EAAEH,MAAM,EAAE;QACZI,OAAO,EAAE,IAAI,CAACE,cAAc;QAC5BD,IAAI,EAAE;MACP;MACAI,OAAO,CAACC,GAAG,CAAC,WAAW,EAAEC,OAAO;MAChC,IAAI,CAACT,KAAK,CAACU,IAAI,CAACD,OAAO;MACvB,IAAI,CAACL,cAAa,GAAI,IAAG;IAChC,CAAC;IACDO,UAAU,CAACC,MAAM,EAAC;MACd,IAAI,CAACZ,KAAK,CAACa,MAAM,CAACD,MAAM,EAAE,CAAC,CAAC;IAChC;EACJ;AACJ","names":["v4","uuidv4","data","todos","id","content","done","newToDoContent","methods","addTodo","console","log","newToDo","push","deleteTodo","itemId","splice"],"sourceRoot":"","sources":["/Users/john-simonbachhuber/Studium/6 Semester/Softwareergonomie/ScrumPrototyp/src/components/JohnToDo.vue"],"sourcesContent":["<template>\r\n<div class=\"todo\">\r\n\r\n    <div class=\"title\">\r\n        John-Todo\r\n    </div>\r\n\r\n        <div class=\"head\">\r\n            <p class=\"expanded\">\r\n                <inp